<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build">
  <Sdk Name="Microsoft.Build.Sql" Version="1.0.0-rc1" />
  <PropertyGroup>
    <Name>ExampleSqlProject</Name>
    <DSP>Microsoft.Data.Tools.Schema.Sql.Sql160DatabaseSchemaProvider</DSP>
    <ModelCollation>1033, CI</ModelCollation>
    <RunSqlCodeAnalysis>true</RunSqlCodeAnalysis>
    <ManagePackageVersionsCentrally>true</ManagePackageVersionsCentrally>
    <SqlCodeAnalysisRules>+!Coding4Fun.SR1001;+!Coding4Fun.SR1002;+!Coding4Fun.SR1003;+!Coding4Fun.SR1004;+!Coding4Fun.SR1005</SqlCodeAnalysisRules>
    <ProjectGuid>{00000000-0000-0000-0000-000000000000}</ProjectGuid>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Coding4Fun.TransactSql.Analyzers" />
  </ItemGroup>
</Project>